{
  "expo": {
    "name": "AlgoTracker",
    "slug": "AlgoTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "algotracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.sohambiswas.AlgoTracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "71cb359c-76e9-44b3-8b5e-ceb1223b1526"
      },
      "clerkPublishableKey": "pk_live_YmVsb3ZlZC1tYXlmbHktODIuY2xlcmsuYWNjb3VudHMuZGV2JA",
      "convexUrl": "https://adamant-bear-321.convex.cloud"
    }
  }
}
